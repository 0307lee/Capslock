<?xml version="1.0"?>
<root>
    <!-- Introduction -->
    <item>
        <name style="important">CapsLock Enhancement</name>
        <appendix>Make CapsLock Great Again!</appendix>
        <appendix>Author: Ruohang Feng(fengruohang@outlook.com)</appendix>
        <appendix>Source: https://github.com/Vonng/Configuration/blob/master/osx/private.xml</appendix>
        <appendix>Platform: Mac OS X darwin64 via Karabiner</appendix>
        <appendix>License: WTFPL</appendix>
        <appendix>Mapping CapsLock to F19 before enabling these functionality.</appendix>
    </item>


    <!-- Hyper declaration -->
    <modifierdef>HYPER</modifierdef>


    <!-- Capslock to Hyper/Language Switch(Recommend) -->
    <item>
        <name style="important">CapsLock(F19) to Hyper/Language Switch</name>
        <appendix>You can only choose one of LanguageSwitch or Escape!</appendix>
        <appendix>Capslock [Hold] : Hyper</appendix>
        <appendix>Capslock [Press] : ^SPace (Switch Language)</appendix>
        <identifier>private.capslock</identifier>
        <autogen>
            --KeyOverlaidModifier--
            KeyCode::F19,
            KeyCode::VK_MODIFIER_HYPER,
            KeyCode::SPACE, ModifierFlag::CONTROL_L
        </autogen>
    </item>


    <!-- Capslock to Hyper/ESC -->
    <item>
        <name style="important">CapsLock(F19) to Hyper/Escape</name>
        <appendix>You can only choose one of LanguageSwitch or Escape!</appendix>
        <appendix>Capslock [Hold] : Hyper</appendix>
        <appendix>Capslock [Press] : Escape</appendix>
        <identifier>private.capslock-escape</identifier>
        <autogen>
            --KeyOverlaidModifier--
            KeyCode::F19,
            KeyCode::VK_MODIFIER_HYPER,
            KeyCode::ESCAPE
        </autogen>
    </item>


    <!-- Hyper Escape -->
    <item>
        <name>Hyper Esc</name>
        <appendix>Hyper Esc : Escape Hyper back to CapsLock itself</appendix>
        <identifier>private.hyper-escape</identifier>
        <autogen>__KeyToKey__ KeyCode::ESCAPE, ModifierFlag::HYPER, KeyCode::CAPSLOCK</autogen>
    </item>


    <!-- Hyper Shifter-->
    <item>
        <name>Hyper Shifter</name>
        <appendix>Hyper 1234567890-=: +⇧</appendix>
        <appendix>Hyper ; : ⇧- `_`</appendix>
